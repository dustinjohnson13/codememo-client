{
  "name": "codememo-client",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dustinjohnson13.github.io/codememo-client/",
  "dependencies": {
    "aws-sdk": "^2.104.0",
    "bootstrap": "4.0.0-alpha.6",
    "dynamodb-local": "^0.0.18",
    "flow-typed": "^2.1.5",
    "freeport": "^1.0.5",
    "react": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.0.11",
    "react-spin": "^0.6.2",
    "react-transition-group": "^1.2.0",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "sequelize": "^4.7.5",
    "spin": "0.0.1",
    "sqlite3": "^3.1.9",
    "uuid": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules/",
      "!src/persist/dynamodb/DynamoDBHelper.js",
      "!src/services/API.js"
    ]
  },
  "jest-junit": {
    "suiteName": "CodeMemo Tests",
    "output": "./gradle-build/test-results/junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "react-scripts test --env=jsdom --coverage --coverageDirectory ./gradle-build/coverage --testResultsProcessor ./node_modules/jest-junit",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "flow-bin": "^0.53.1",
    "gh-pages": "^1.0.0",
    "jest": "21.0.0-beta.1",
    "jest-cli": "^20.0.4",
    "jest-junit": "^2.1.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3"
  }
}
